{
  "k":"Memcached",
  "t":"macOS 上安裝 Memcached",
  "i":"2020-03-17",
  "a":[
    {
      "e":"h2",
      "t":"安裝方法有兩種："
    },
    {
      "e":"ol",
      "s":[
        {
          "e":"li",
          "t":"指令安裝(推薦)"
        },
        {
          "e":"li",
          "t":"步驟安裝"
        }
      ]
    },
    {
      "e":"h2",
      "t":"指令安裝(推薦)"
    },
    {
      "e":"ul",
      "s":[
        {
          "e":"li",
          "s":[
            {
              "e":"span",
              "t":"指令 "
            },
            {
              "e":"code",
              "t":"brew install memcached"
            }
          ]
        }
      ]
    },
    {
      "e":"h2",
      "t":"步驟安裝(不推薦，因為步驟複雜了點)"
    },
    {
      "e":"ul",
      "s":[
        {
          "e":"li",
          "s":[
            {
              "e":"span",
              "t":"Memcached 相依於 "
            },
            {
              "e":"b",
              "t":"libevent"
            },
            {
              "e":"span",
              "t":", 所以在安裝之前要先安裝 "
            },
            {
              "e":"a",
              "attr":{
                "href":"http://libevent.org",
                "target":"_blank"
              },
              "t":"libevent"
            },
            {
              "e":"span",
              "t":"。"
            },
            {
              "e":"ul",
              "s":[
                {
                  "e":"li",
                  "s":[
                    {
                      "e":"span",
                      "t":"安裝 libevent"
                    },
                    {
                      "e":"ul",
                      "s":[
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"選擇暫存資料夾 "
                            },
                            {
                              "e":"code",
                              "t":"cd /tmp"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"下載檔案 "
                            },
                            {
                              "e":"code",
                              "t":"wget https://github.com/downloads/libevent/libevent/libevent-2.0.18-stable.tar.gz"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"解開壓縮 "
                            },
                            {
                              "e":"code",
                              "t":"tar zxvf libevent-2.0.18-stable.tar.gz"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"進去資料夾 "
                            },
                            {
                              "e":"code",
                              "t":"cd libevent-2.0.18-stable"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"執行指令 "
                            },
                            {
                              "e":"code",
                              "t":"./configure"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"執行指令 "
                            },
                            {
                              "e":"code",
                              "t":"make"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"執行指令 "
                            },
                            {
                              "e":"code",
                              "t":"sudo make install"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                },
                {
                  "e":"li",
                  "s":[
                    {
                      "e":"span",
                      "t":"安裝 Memcached"
                    },
                    {
                      "e":"ul",
                      "s":[
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"選擇暫存資料夾 "
                            },
                            {
                              "e":"code",
                              "t":"cd /tmp"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"下載檔案 "
                            },
                            {
                              "e":"code",
                              "t":"wget http://memcached.googlecode.com/files/memcached-1.4.13.tar.gz"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"解開壓縮 "
                            },
                            {
                              "e":"code",
                              "t":"cd memcached-1.4.13"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"執行指令 "
                            },
                            {
                              "e":"code",
                              "t":"./configure"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"執行指令 "
                            },
                            {
                              "e":"code",
                              "t":"make"
                            }
                          ]
                        },
                        {
                          "e":"li",
                          "s":[
                            {
                              "e":"span",
                              "t":"執行指令 "
                            },
                            {
                              "e":"code",
                              "t":"sudo make install"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
