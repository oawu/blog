            },
            {
              "e":"span",
              "t":"，"
            },
            {
              "e":"code",
              "t":"-p"
            },
            {
              "e":"span",
              "t":" 代表 "
            },
            {
              "e":"b",
              "t":"port"
            },
            {
              "e":"span",
              "t":"。"
            }
          ]
        },
        {
          "e":"li",
          "s":[
            {
              "e":"span",
              "t":"背景執行指令 "
            },
            {
              "e":"code",
              "t":"memcached -d -p 8000"
            },
            {
              "e":"span",
              "t":"，加個 "
            },
            {
              "e":"code",
              "t":"-d"
            },
            {
              "e":"span",
              "t":" 即可背景。"
            }
          ]
        }
